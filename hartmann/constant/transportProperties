FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

rho             [1 -3 0 0 0 0 0] 1;//密度

nu              [0 2 -1 0 0 0 0] 1;//動粘性率

mu              [1 1 -2 0 0 -2 0] 1;//粘性係数

sigma           [-1 -3 3 0 0 2 0] 1;//表面張力

